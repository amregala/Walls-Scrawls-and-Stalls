<%- include('./partials/header.ejs') %>
<%- include('./partials/add-new.ejs') %>

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>WSS | Edit Page</title>
</head>
<body>
    <div class="add-box">
        <h2>Edit Wall, Scrawl or Stall</h2>
        <form action="/walls/<%= wall.id %>?_method=PUT" method="POST">
          
          <div class="input-box">
            <input type="text" name="year" value="<%= wall.year %>">
            
           
          </div>
        
          <div class="input-box">
            <input type="text" name="location" value="<%= wall.location %>"/>    
           
            
          </div>
          <div class="input-box">
            <input type="text" name="artist" value="<%= wall.artist %>"/>    
            
            <br>
          </div>
        
          <input type="submit" value="Submit Edit"/>
        </form>
        
        </div>
        
          </body>
        </html>
</body>
</html>